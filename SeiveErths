#include <bits/stdc++.h>
using namespace std;

const int N = 1e5 + 10;
vector<int> prime(N, 0); // 0 = prime, 1 = not prime

void sieve(int x) {

    prime[0] = prime[1] = 1;

    for (int i = 2; i * i < N; i++) {
        if (prime[i]) continue;

        for (int j = i * i; j < N; j += i) {
            prime[j] = 1;
        }
    }

    if (prime[x] == 0)
        cout << "Yes, " << x << " is Prime\n";
    else
        cout << x << " is Not Prime\n";
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);

    int x;
    cin >> x;
    
    sieve(x);
    return 0;
}
